{% macro url_button(classes, page_button) %}
    <div class="{{ classes }} w3-grey">
        <a href="{{ page_button.url }}" class="w3-button w3-teal">
            <span class="w3-large">{{ page_button.name }}</span><br>
            <span class="w3-small">{{ page_button.url }}</span>
        </a>
    </div>
{% endmacro -%}